{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ],
  "crons": [
    {
      "path": "/api/cron-job",
      "schedule": "*/1 * * * *",
      "timezone": "Asia/Kolkata"
    }
  ],
  "buildCommand": ""
}
